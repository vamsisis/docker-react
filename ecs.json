{
  "name": "nginx-exporter",
  "image": "nginx/nginx-prometheus-exporter:latest",
  "essential": false,
  "portMappings": [
    {
      "containerPort": 9113,
      "protocol": "tcp"
    }
  ],
  "command": [
    "-nginx.scrape-uri=https://localhost:443/rps/basic_status",
    "-nginx.ssl-verify=false",
    "-web.listen-address=:9113"
  ]
}
